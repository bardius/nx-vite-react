{
  "name": "@bardius/nx-vite-react-monorepo",
  "version": "0.0.0",
  "author": "George Bardis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bardius/nx-vite-react/issues"
  },
  "homepage": "https://github.com/bardius/nx-vite-react/blob/main/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bardius/nx-vite-react.git"
  },
  "private": true,
  "scripts": {
    "setup": "npm i --include=optional",
    "setup:ci": "npm ci && npm i @nx/nx-linux-x64-gnu",
    "clean": "nx run-many -t clean --parallel=false && rimraf -rf ./dist && nx reset && git clean -xdf node_modules",
    "clean:cache": "rimraf -rf ./node_modules/.cache && rimraf -rf ./.nx/cache",
    "prepare": "husky install",
    "pre-commit": "npm run test && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "commit": "git-cz",
    "lint:ci": "nx run-many -t lint --configuration=ci --parallel=false --verbose",
    "lint": "nx run-many -t lint --parallel=false --verbose",
    "lint:styles": "nx run-many -t lint:styles",
    "lint:styles:ci": "nx run-many -t lint:styles:ci",
    "check:circular": "nx run-many -t check:circular --parallel=false --verbose",
    "check:support": "NODE_ENV=production npx browserslist --mobile-to-desktop",
    "check:duplicates": "nx run-many -t check:duplicates --parallel=false --verbose",
    "check:security": "npm audit --recursive --audit-level=high",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "test": "nx run-many -t test --verbose",
    "test:ci": "nx run-many -t test --parallel=false --configuration=ci --runInBand --maxWorkers=1 --forceExit",
    "e2e": "nx run-many -t e2e",
    "graph": "nx graph",
    "build": "npx nx run-many -t build",
    "storybook": "nx run common-ui:storybook",
    "storybook:test": "nx run common-ui:test-storybook"
  },
  "dependencies": {
    "@fontsource/open-sans": "^5.0.22",
    "@fontsource/pt-mono": "^5.0.8",
    "@salt-ds/core": "^1.13.4",
    "@salt-ds/icons": "^1.9.1",
    "@salt-ds/lab": "^1.0.0-alpha.26",
    "@salt-ds/theme": "^1.11.0",
    "@swc/helpers": "~0.5.2",
    "msw": "^2.0.14",
    "ramda": "^0.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-router-dom": "^6.21.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/config-conventional": "^18.4.4",
    "@commitlint/config-nx-scopes": "^18.4.4",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@nx/cypress": "17.2.8",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/jest": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/react": "17.2.8",
    "@nx/storybook": "17.2.8",
    "@nx/vite": "17.2.8",
    "@nx/web": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@storybook/addon-a11y": "^7.6.8",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.6.8",
    "@storybook/addon-mdx-gfm": "^7.6.8",
    "@storybook/addon-storysource": "^7.6.8",
    "@storybook/addons": "^7.6.8",
    "@storybook/core-server": "^7.5.3",
    "@storybook/jest": "^0.2.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@testing-library/react": "14.1.2",
    "@types/jest": "^29.4.0",
    "@types/node": "20.11.0",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "~0.34.6",
    "@vitest/ui": "~0.34.6",
    "babel-jest": "^29.4.1",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.4",
    "cypress": "^13.0.0",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-cypress": "^2.13.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^29.4.1",
    "jest-junit": "^16.0.0",
    "jest-sonar": "^0.2.16",
    "jscpd": "^3.5.10",
    "jsdom": "~22.1.0",
    "lint-staged": "^15.2.0",
    "madge": "^6.1.0",
    "nx": "17.2.8",
    "prettier": "^2.6.2",
    "rimraf": "^5.0.5",
    "sass": "^1.55.0",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "~2.3.0",
    "vitest": "~0.34.6"
  },
  "optionalDependencies": {
    "@nx/nx-darwin-arm64": "17.2.8",
    "@nx/nx-darwin-x64": "17.2.8",
    "@nx/nx-linux-x64-gnu": "17.2.8",
    "@nx/nx-win32-x64-msvc": "17.2.8",
    "@rollup/rollup-linux-x64-gnu": "4.9.5",
    "@rollup/rollup-win32-x64-msvc": "4.9.5"
  },
  "lint-staged": {
    "*.{json,md,ts,tsx,sass,css}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./scripts/commitizen"
    }
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 edge version",
      "last 1 safari version"
    ]
  }
}
